<?xml version="1.0" encoding="utf-8"?>
<bundle name="Akismet">
  <domain name="akismet">
    <project name="Akismet" slug="akismet">
      <!-- glotpress doesn't ship with languages directory, so saving in global lang dir -->
      <template>
        <file base="WP_LANG_DIR">plugins/akismet.pot</file>
      </template>
    </project>
  </domain>
  <exclude>
    <directory>_inc</directory>
  </exclude>
</bundle>
