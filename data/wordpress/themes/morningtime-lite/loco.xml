    <project name="MorningTime Lite" slug="morningtime-lite">
      <source>
        <directory>.</directory>
        <exclude>
          <directory>languages</directory>
        </exclude>
      </source>
      <target>
        <directory>languages</directory>
      </target>
      <template>
        <file>languages/en_US.po</file>
